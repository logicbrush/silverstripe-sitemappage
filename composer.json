{
	"name": "logicbrush/silverstripe-sitemappage",
	"description": "A module for the SilverStripe CMS which produces a page listing every (public) page on your site.",
	"homepage": "https://www.logicbrush.com/specialities/silverstripe-development/",
	"type": "silverstripe-vendormodule",
	"keywords": [
		"silverstripe",
		"sitemap"
	],
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "John Learn",
		"email": "jflearn@logicbrush.com"
	}],
	"require": {
		"silverstripe/cms": "^4.0"
	},
	"extra": {
		"expose": [
			"images"
		]
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7"
	},
	"autoload": {
		"psr-4": {
			"Logicbrush\\SiteMapPage\\": "src/",
			"Logicbrush\\SiteMapPage\\Tests\\": "tests/"
		}
	}
}
